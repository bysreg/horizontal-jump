{
	"name": "hjump",
	"displayName": "Horizontal Jump",
	"description": "Jumps to character after the next whitespace in the line.",
	"publisher": "hibe",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:hjump.jumpRight",
		"onCommand:hjump.jumpLeft"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "hjump.jumpRight",
				"title": "Horizontal Jump: Jump Right"
			},
			{
				"command": "hjump.jumpLeft",
				"title": "Horizontal Jump: Jump Left"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
